@model IEnumerable<FaultTrage_Test2.TroubleshooterManager>

@{
    ViewBag.Title = "Troubleshooter Manager";
}

<h2>Troubleshooter Manager</h2>

<p>
    @Html.ActionLink("Create New Troubleshooter Manager", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ManagerName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ManagerDesignation)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ManagerName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ManagerDesignation)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TroubleshooterManagerId }) |
            @Html.ActionLink("Details", "Details", new { id=item.TroubleshooterManagerId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.TroubleshooterManagerId })
        </td>
    </tr>
}

</table>
